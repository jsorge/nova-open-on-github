{
  "identifier": "org.OpenOnGithub",
  "name": "Open On Github",
  "organization": "MyOrganization",
  "description": "Lorem ipsum, dolor sit amet.",
  "version": "1.0",
  "categories": ["commands"],

  "main": "main.js",

  "activationEvents": ["onLanguage:html"],

  "entitlements": {
    "filesystem": "read",
    "process": true
  },

  "commands": {
    "editor": [
      {
        "title": "Open file on GitHub",
        "command": "open-on-github.openCurrentFile"
      }
    ]
  },

  "config": [
    {
      "key": "open-on-github.foobar",
      "title": "Foo the bar",
      "type": "boolean",
      "default": false
    }
  ]
}
